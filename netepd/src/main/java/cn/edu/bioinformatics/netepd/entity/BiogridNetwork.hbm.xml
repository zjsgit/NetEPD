<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="cn.edu.bioinformatics.netepd.entity.BiogridNetwork" table="biogridnetwork">
        <id name="id" type="java.lang.Integer">
            <column name="INTERACTION_ID" />
            <generator class="native" />
        </id>
        <property name="biogridAid" type="java.lang.String">
            <column name="BIOGRID_ID_A" />
        </property>
        <property name="biogridBid" type="java.lang.String">
            <column name="BIOGRID_ID_B" />
        </property>
        <property name="officialA" type="java.lang.String">
            <column name="OFFICIAL_SYMBOL_A" />
        </property>
        <property name="officialB" type="java.lang.String">
            <column name="OFFICIAL_SYMBOL_B" />
        </property>
        <property name="organismAid" type="java.lang.String">
            <column name="ORGANISM_ID_A" />
        </property>
        <property name="organismBid" type="java.lang.String">
            <column name="ORGANISM_ID_B" />
        </property>
    </class>
</hibernate-mapping>
