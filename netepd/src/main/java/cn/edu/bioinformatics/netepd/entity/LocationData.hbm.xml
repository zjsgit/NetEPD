<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="cn.edu.bioinformatics.netepd.entity.LocationData" table="locationdata">
        <id name="id" type="java.lang.Integer">
            <column name="PROTEIN_ID" />
            <generator class="native" />
        </id>
        <property name="proteinName" type="java.lang.String">
            <column name="PROTEIN_NAME" />
        </property>
        <property name="proteinLocation" type="java.lang.String">
            <column name="PROTEIN_LOCATION" />
        </property>
        <property name="organismId" type="java.lang.String">
            <column name="ORGANISM_ID" />
        </property>
    </class>
</hibernate-mapping>
