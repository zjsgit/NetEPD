<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="cn.edu.bioinformatics.netepd.entity.MintNetwork" table="mintnetwork">
        <id name="id" type="java.lang.Integer">
            <column name="INTERACTION_ID" />
            <generator class="native" />
        </id>
        <property name="interactorAid" type="java.lang.String">
            <column name="INTERACTOR_ID_A" />
        </property>
        <property name="interactorBid" type="java.lang.String">
            <column name="INTERACTOR_ID_B" />
        </property>
        <property name="organismAid" type="java.lang.String">
            <column name="ORGANISM_ID_A" />
        </property>
        <property name="organismBid" type="java.lang.String">
            <column name="ORGANISM_ID_B" />
        </property>
        <property name="hostId" type="java.lang.String">
            <column name="HOST_ORGANISM_ID" />
        </property>
    </class>
</hibernate-mapping>
